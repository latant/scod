{
  "compilerOptions": {
    "incremental": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "ES2019",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "outDir": "dist",
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "lib": ["ES2021.String"]
  },
}
